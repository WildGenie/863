    <SolidColorBrush x:Key="colorTransWhite" Color="#FFFFFF" Opacity="0.5"></SolidColorBrush>
    <SolidColorBrush x:Key="colorDarkWhite" Color="Silver" Opacity="1"></SolidColorBrush>

    <SolidColorBrush x:Key="colorTransparent" Color="#FFFFFF" Opacity="0.3"></SolidColorBrush>

    <SolidColorBrush x:Key="colorBlack" Color="DimGray" Opacity="1"/>
    <SolidColorBrush x:Key="colorTransBlack" Color="DarkGray" Opacity="0.7"/>
    <SolidColorBrush x:Key="colorLightBlack" Color="DarkGray" />
