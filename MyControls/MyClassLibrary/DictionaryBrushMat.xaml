    <!--电厂材质-->
    <MaterialGroup x:Key="matHDNormal">
        <DiffuseMaterial >
            <DiffuseMaterial.Brush>
                <ImageBrush ImageSource="/MyClassLibrary;component/Images/plantmatNormal.jpg" />
            </DiffuseMaterial.Brush>
        </DiffuseMaterial>
        <SpecularMaterial SpecularPower="90" Brush="{StaticResource colorLightWhite}"/>
    </MaterialGroup>
    <MaterialGroup x:Key="matHDHighLight">
        <DiffuseMaterial AmbientColor="Red">
            <DiffuseMaterial.Brush>
                <ImageBrush ImageSource="/MyClassLibrary;component/Images/plantmatNormal.jpg" />
            </DiffuseMaterial.Brush>
        </DiffuseMaterial>
        <SpecularMaterial SpecularPower="90" Brush="{StaticResource colorLightWhite}"/>
    </MaterialGroup>

    <MaterialGroup x:Key="matSDNormal">
