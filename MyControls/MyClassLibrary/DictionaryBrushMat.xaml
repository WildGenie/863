    <!--与3D材质配套SolidColor画刷-->
    <SolidColorBrush x:Key="colorRed" Color="Red" Opacity="1" />
    <SolidColorBrush x:Key="colorTransRed" Color="#FFFF8080" Opacity="0.7" />
    <SolidColorBrush x:Key="colorLightRed" Color="#FFFF8080" />
    <SolidColorBrush x:Key="colorDarkRed" Color="DarkRed" Opacity="1" />

    <SolidColorBrush x:Key="colorYellow" Color="Chocolate" Opacity="1"></SolidColorBrush>
    <SolidColorBrush x:Key="colorTransYellow" Color="Orange" Opacity="0.7"></SolidColorBrush>
    <SolidColorBrush x:Key="colorLightYellow" Color="Orange" Opacity="1"></SolidColorBrush>
    <SolidColorBrush x:Key="colorDarkYellow" Color="OrangeRed" Opacity="1"></SolidColorBrush>

    <SolidColorBrush x:Key="colorBlue" Color="Blue" Opacity="1"></SolidColorBrush>
    <SolidColorBrush x:Key="colorTransBlue" Color="#0099FF" Opacity="0.7"></SolidColorBrush>
    <SolidColorBrush x:Key="colorLightBlue" Color="#6666FF" Opacity="1"></SolidColorBrush>
