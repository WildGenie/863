    <SolidColorBrush x:Key="colorDarkGreen" Color="DarkGreen" Opacity="1"></SolidColorBrush>

    <SolidColorBrush x:Key="colorWhite" Color="Gainsboro" Opacity="1"></SolidColorBrush>
